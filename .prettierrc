{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "experimentalTernaries": true,
    "experimentalOperatorPosition": "start",
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "objectWrap": "preserve",
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 80,
    "singleAttributePerLine": true,
    "embeddedLanguageFormatting": "auto"
}